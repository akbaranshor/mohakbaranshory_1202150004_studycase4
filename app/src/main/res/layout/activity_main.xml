<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.android.asynctask.MainActivity"
    android:orientation="vertical"
    android:gravity="center">
    <Button
        android:id="@+id/list_nama_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="List Nama Mahasiswa"
        android:onClick="listMhs"/>

    <Button
        android:id="@+id/cari_gbr_btn"
        android:onClick="cariGbr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pencari Gambar"
        android:layout_marginTop="16dp"/>
</LinearLayout>
